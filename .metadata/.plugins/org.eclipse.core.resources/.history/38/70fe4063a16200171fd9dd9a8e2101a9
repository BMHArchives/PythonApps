print("Welcome to Tic Tac Toe Game")
p1Name = ""
p2Name = ""
def ShowBoard():
    print("_ _1 _ _| _ _2 _ _ | _ _3 _ _\n_ _4 _ _| _ _5 _ _ | _ _6 _ _\n_ _7 _ _| _ _8 _ _ | _ _9 _ _")
    
ShowBoard()
print("This is a two player game.") 
p1Name = input("Player 1, please enter your name")
p2Name = input("Player 2, please enter your name")

print('Welcome,{}. You are player 1'.format(p1Name))
print('Welcome,{}. You are player 2'.format(p2Name))